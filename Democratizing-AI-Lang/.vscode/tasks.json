{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "cargo",
            "args": [
                "build",
                "--all-features"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "build-release",
            "type": "shell",
            "command": "cargo",
            "args": [
                "build",
                "--release",
                "--all-features"
            ],
            "group": "build",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "test",
            "type": "shell",
            "command": "cargo",
            "args": [
                "test",
                "--all-features"
            ],
            "group": {
                "kind": "test",
                "isDefault": true
            },
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "check",
            "type": "shell",
            "command": "cargo",
            "args": [
                "check",
                "--all-features"
            ],
            "group": "build",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "clippy",
            "type": "shell",
            "command": "cargo",
            "args": [
                "clippy",
                "--all-targets",
                "--all-features",
                "--",
                "-D",
                "warnings"
            ],
            "group": "build",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "fmt",
            "type": "shell",
            "command": "cargo",
            "args": [
                "fmt",
                "--all"
            ],
            "group": "build",
            "problemMatcher": []
        },
        {
            "label": "doc",
            "type": "shell",
            "command": "cargo",
            "args": [
                "doc",
                "--no-deps",
                "--document-private-items"
            ],
            "group": "build",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "bench",
            "type": "shell",
            "command": "cargo",
            "args": [
                "bench",
                "--all-features"
            ],
            "group": "test",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "build-cuda-example",
            "type": "shell",
            "command": "cargo",
            "args": [
                "build",
                "--example=cuda_acceleration",
                "--features=cuda"
            ],
            "group": "build",
            "problemMatcher": [
                "$rustc"
            ]
        },
        {
            "label": "clean",
            "type": "shell",
            "command": "cargo",
            "args": [
                "clean"
            ],
            "group": "build",
            "problemMatcher": []
        },
        {
            "label": "update-deps",
            "type": "shell",
            "command": "cargo",
            "args": [
                "update"
            ],
            "group": "none",
            "problemMatcher": []
        },
        {
            "label": "audit",
            "type": "shell",
            "command": "cargo",
            "args": [
                "audit"
            ],
            "group": "none",
            "problemMatcher": []
        }
    ]
}
